<%= form_for [:admin, song] do |f| %>
  <%= render "shared/error_messages", object: f.object %>
  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %>
  <%= f.label :artist %>
  <%= f.text_field :artist, class: "form-control" %>
  <%= f.label :category_id %>
  <%= f.select :category_id, load_all_cate,
               {include_blank: false},
               {class: "form-control"} %>
  <%= f.label :description %>
  <%= f.text_area :description, class: "form-control" %>
  <%= f.label :song_file %>
  <%= f.file_field :song_file, class: "form-control" %>
  <%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>
